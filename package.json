{
  "name": "@bicou/nuxt-fontawesome",
  "version": "2.0.2",
  "description": "Minimal FontAwesome module for Nuxt 3",
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/gbicou/nuxt-fontawesome.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/module.mjs"
  },
  "main": "./dist/module.mjs",
  "types": "./dist/module.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "nuxt-module-build prepare",
    "build": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/vue-fontawesome": "3.0.8",
    "@nuxt/kit": "3.17.6"
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@nuxt/devtools": "2.6.2",
    "@nuxt/eslint-config": "1.5.0",
    "@nuxt/module-builder": "1.0.1",
    "@nuxt/schema": "3.17.6",
    "@nuxt/test-utils": "3.19.2",
    "@types/node": "22.16.0",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.30.1",
    "happy-dom": "18.0.1",
    "nuxt": "3.17.6",
    "typescript": "5.8.3",
    "vitest": "3.2.4",
    "vue-tsc": "2.2.12"
  },
  "packageManager": "pnpm@10.12.2+sha512.a32540185b964ee30bb4e979e405adc6af59226b438ee4cc19f9e8773667a66d302f5bfee60a39d3cac69e35e4b96e708a71dd002b7e9359c4112a1722ac323f",
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild"
    ]
  }
}
